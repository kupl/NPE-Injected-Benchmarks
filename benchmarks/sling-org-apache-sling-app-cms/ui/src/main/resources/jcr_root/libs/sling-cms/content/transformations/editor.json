            "template": {
                "jcr:primaryType": "nt:unstructured",
                "label": "Transformation",
                "prefix": "/cms/transformations/create.html"
            }
        }
    },
    "contenttable": {
        "jcr:primaryType": "nt:unstructured",
        "sling:resourceType": "sling-cms/components/cms/contenttable",
        "appendSuffix": "/transformations",
        "columns": {
            "jcr:primaryType": "nt:unstructured",
            "name": {
                "jcr:primaryType": "nt:unstructured",
