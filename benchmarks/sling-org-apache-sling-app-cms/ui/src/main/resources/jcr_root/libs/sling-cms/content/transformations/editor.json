                    },
                    "transformationName": {
                        "jcr:primaryType": "nt:unstructured",
                        "sling:resourceType": "sling-cms/components/cms/columns/text",
                        "property": "name"
                    },
                    "lastModified": {
                        "jcr:primaryType": "nt:unstructured",
                        "sling:resourceType": "sling-cms/components/cms/columns/lastmodified",
