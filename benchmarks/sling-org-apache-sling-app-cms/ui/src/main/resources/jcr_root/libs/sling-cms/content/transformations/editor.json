            },
            "lastModified": {
                "jcr:primaryType": "nt:unstructured",
                "title": "Last Modified"
            },
            "actions": {
                "jcr:primaryType": "nt:unstructured",
                "title": "Actions"
            }
        },
        "types": {
            "jcr:primaryType": "nt:unstructured",
            "nt:unstructured": {
                "jcr:primaryType": "nt:unstructured",
                "columns": {
                    "jcr:primaryType": "nt:unstructured",
                    "name": {
                        "jcr:primaryType": "nt:unstructured",
                        "sling:resourceType": "sling-cms/components/cms/columns/name",
